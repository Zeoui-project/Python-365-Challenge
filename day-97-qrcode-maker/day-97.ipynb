   "source": [
    "import pyqrcode\n",
    "from PIL import Image\n",
    "link = input(\"Enter anything to generate QR : \")\n",
    "qr_code = pyqrcode.create(link)\n",
    "qr_code.png('qr.png', scale=5)\n",
    "Image.open('qr.png')"
   ]
