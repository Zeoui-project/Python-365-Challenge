    "import matplotlib.pyplot as pyplot\n",
    "labels = ('Python', 'Java', 'Scala', 'C#')\n",
    "sizes = [45, 30, 15, 10]\n",
    "pyplot.pie(sizes, labels=labels, autopct='%1.f%%', counterclock=False, startangle=105)\n",
    "\n",
    "pyplot.show()"
