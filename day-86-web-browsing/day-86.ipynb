   "source": [
    "import webbrowser\n",
    "Url = input(\"Enter a url : \")\n",
    "webbrowser.open(Url)\n",
    "\n",
    "# Open the page in a new browser window.\n",
    "webbrowser.open_new('Url')\n",
    "\n",
    "# Open the page in a new browser tab\n",
    "webbrowser.open_new_tab('Url')"
   ]
