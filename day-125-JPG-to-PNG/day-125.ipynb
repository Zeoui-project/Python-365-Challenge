    "#PNG to JPG\n",
    "from PIL import Image\n",
    "im = Image.open(\"test.png\")\n",
    "rgb_im = im.convert('RGB')\n",
    "rgb_im.save('PNGtoJPGtest.jpg')"
   ]
  },
